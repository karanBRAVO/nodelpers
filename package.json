    "format": "prettier --write .",
    "prepare": "husky"
  },
  "keywords": [
    "node",
    "npm",
    "typescript",
    "utility",
    "helper",
    "util"
  ],
  "author": "Karan Yadav",
  "homepage": "https://github.com/karanBRAVO/nodelpers/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/karanBRAVO/nodelpers/issues"
  },
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karanBRAVO/nodelpers.git"
  },
  "description": "This project contains helper functions for nodejs.",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "chai": "^5.1.2",
    "chai-as-promised": "^8.0.1",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "mocha": "^11.1.0",
    "prettier": "^3.4.2",
    "sinon": "^19.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.1"
  },
  "dependencies": {
    "chalk": "^5.4.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,md}": "prettier --write"
  }
